    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-cssmin": "0.1.7",
    "gulp-load-plugins": "^1.2.0",
    "gulp-sass": "^2.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    
    "jasmine": "2.4.1",
    "jasmine-core": "2.4.1",
    "jshint": "2.9.1",
    "jshint-stylish": "2.1.0",
