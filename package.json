{
  "name": "tinkertron",
  "version": "0.0.1",
  "description": "A tinkering in Electron",
  "main": "main.js",
  
  "scripts": {
    "postinstall": "bower install",
    "prestart": "npm install",
    "start": "concurrent --kill-others \"gulp sass\" \"electron ./src/main.js\" \"gulp sasswatch\""
